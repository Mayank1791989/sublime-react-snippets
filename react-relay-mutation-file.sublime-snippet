  <content><![CDATA[
import _omit from 'lodash/object/omit';
import _pick from 'lodash/object/pick';
import isFile from 'utils/isFile';
getVariables() {
  return _omit(this.props.input, isFile);
}

getFiles() {
  return _pick(this.props.input, isFile);
}
